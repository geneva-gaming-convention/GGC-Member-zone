<div class="modal fade" id="sharing_modal" tabindex="-1" role="dialog" aria-labelledby="sharing_modal_Label" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
          <span class="pficon pficon-close"></span>
        </button>
        <h4 class="modal-title" id="sharing_modal_Label">Share</h4>
      </div>
      <div class="modal-body">
        <p>
          <%= data %>
        </p>
        <pre style="color: #0088ce; padding-top:3em; padding-bottom:0em;"><%= link %>

        </pre>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
      </div>
    </div>
  </div>
</div>
